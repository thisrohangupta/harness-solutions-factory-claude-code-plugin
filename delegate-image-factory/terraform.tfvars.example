# Terraform TFVARS Sample
# ---------
# [Optional] Enter the Harness Platform URL.  Defaults to Harness SaaS URL
harness_platform_url = "https://app.harness.io/gateway"

# [Required] Enter the Harness Platform Account Number
harness_platform_account = null # If Not passed, then the ENV HARNESS_ACCOUNT_ID will be used

# [Required] Provide an existing Harness Platform key secret reference.  Must exist before execution
existing_harness_platform_key_ref = # Required

# [Required] Provide an existing organization reference ID.  Must exist before execution
organization_id = # Required

# [Required] Provide an existing project reference ID.  Must exist before execution
project_id = # Required

# [Optional] The source CodeBase Repository Name
git_repository_name = "harness-delegate-setup"

# [Optional] When provided, the existing Git connector will be used. When 'null' a custom Harness Code Repository will be added into the project.
git_connector_ref = null

# [Required] Enter the existing Kubernetes connector if local K8s execution should be used when running the Execution pipeline.  Must exist before execution
kubernetes_connector = # Required

# [Optional] Enter the existing Kubernetes namespace if local K8s execution should be used when running the Execution pipeline.  Must exist before execution
kubernetes_namespace = "default"

# [Optional] Optional Kubernetes Node Selectors
kubernetes_node_selectors = {}

# [Optional] Enter an existing Container Registry connector to use which overrides the default connector.  Must exist before execution
kubernetes_override_image_connector = null

# [Required] Docker Registry Name to which the container will be published
container_registry_name = # Required

# [Required] Existing Docker Registry Connector Id.  Must exist before execution
container_registry_connector_id = # Required

# [Optional] Include the Image Test Scan
include_image_test_scan = false

# [Optional] Include Harness Software Bill of Materials
include_image_sbom = false

# [Optional] Container Registry Type
container_registry_type = "docker"

# [Optional] Harness STO Scanner Type
sto_scanner_type = "aqua_trivy"
